

	<!-- Product -->
	<section class="bg0 p-t-23 p-b-130">
		<div class="container">
			<div class="p-b-10">
				<h3 class="ltext-103 cl5">
					Product Overview
				</h3>
			</div>

			<div class="flex-w flex-sb-m p-b-52">
				<div class="flex-w flex-l-m filter-tope-group m-tb-10">
					<button class="stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5 pt-5 how-active1" data-filter="*">
						All Products
				
				</div>

				
			</div> 

			<div class="row isotope-grid">
				{{#each products}}
				<div class="col-sm-6 col-md-4 col-lg-3 p-b-35 isotope-item women">
					<!-- Block2 -->
					<div class="block2" id="wishimage{{this.product._id}}">
						<div class="block2-pic hov-img0 " >
							<img style="height: 300px;" src="/product-images/{{this.product._id}}-0.jpg" alt="IMG-PRODUCT" >
							<a href="/productdetails/{{this.product._id}}"
                class="block2-btn flex-c-m stext-103 cl2 size-102 bg0 bor2 hov-btn1 p-lr-15 trans-04" >
                Quick View
              </a>
							
						</div>

						<div class="block2-txt flex-w flex-t p-t-14">
							<div class="block2-txt-child1 flex-col-l ">
								<a href="product-detail.html" class="stext-104 cl4 hov-cl1 trans-04 js-name-b2 p-b-6">
									{{product.Name}}
								</a>

								<span class="stext-105 cl3">
									{{product.Price}}
								</span>
							</div>
							<div class="block2-txt-child2 flex-r p-t-3">
								
                                 <button id="wishbox{{this.product._id}}" class="btn-addwish-b11" onclick="removeWish('{{this.product._id}}')"><i class="fa fa-heart" style="color:crimson;"></i></button>

							</div>
						</div>
					</div>					
				</div>	
				{{/each}}			
			</div>

		
		</div>
	</section>


	<!-- Footer -->
	

